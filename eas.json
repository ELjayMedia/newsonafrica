{
  "cli": {
    "version": ">= 5.9.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production"
    },
    "android.production": {
      "channel": "production"
    },
    "ios.production": {
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "production"
      },
      "ios": {
        "appleId": "$APPLE_ID",
        "ascAppId": "6736284937",
        "appleTeamId": "$APPLE_TEAM_ID"
      }
    },
    "android.production": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "production"
      }
    },
    "ios.production": {
      "ios": {
        "appleId": "$APPLE_ID",
        "ascAppId": "6736284937",
        "appleTeamId": "$APPLE_TEAM_ID"
      }
    }
  }
}
