{
  "expo": {
    "name": "News On Africa",
    "slug": "news-on-africa-pwa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.newsonafrica.app",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.newsonafrica.app",
      "versionCode": 1,
      "permissions": ["INTERNET", "ACCESS_NETWORK_STATE", "WAKE_LOCK"],
      "softwareKeyboardLayoutMode": "pan",
      "allowBackup": false,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["@react-native-firebase/app", "@react-native-firebase/analytics", "expo-updates"],
    "extra": {
      "eas": {
        "projectId": "3695ce73-c873-4906-9e05-6f302ce20c68"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/3695ce73-c873-4906-9e05-6f302ce20c68"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
